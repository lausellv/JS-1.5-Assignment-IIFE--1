{
    "env": {
        "env": {
            "es6": true,
            "browser": true
        },
        "extends": [
            "eslint:recommended"
        ],
        "rules": {
            "no-console": [
                "error",
                {
                    "allow": [
                        "warn",
                        "error"
                    ]
                }
            ],
            "quotes": [
                "error",
                "single"
            ],
            "no-undef": ["off",
                "jQuery"]
        }
    }
}